{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"username": {
			"type": "string",
			"minLength": 4,
			"maxLength": 32
		},
		"firstName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 32
		},
		"lastName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 32
		},
		"birthdateYear": {
			"type": "integer",
			"minimum": 1900,
			"maximum": 2022
		},
		"birthdateMonth": {
			"type": "integer",
			"minimum": 1,
			"maximum": 12
		},
		"birthdateDay": {
			"type": "integer",
			"minimum": 1,
			"maximum": 31
		},
		"email": {
			"type": "string",
			"minLength": 6,
			"maxLength": 60,
			"format":"email"
		},
		"password": {
			"type": "string",
			"minLength": 6,
			"maxLength": 20
		}
	},
	"additionalProperties": false,
	"required": [
		"username",
		"firstName",
		"lastName",
		"birthdateYear",
		"birthdateMonth",
		"birthdateDay",
		"email",
		"password"
	]
}