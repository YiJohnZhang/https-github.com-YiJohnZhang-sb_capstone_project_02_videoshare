{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"username": {
			"type": "string",
			"minLength": 4,
			"maxLength": 32
		},
		"password": {
			"type": "string",
			"minLength": 6,
			"maxLength": 20
		}
	},
	"additionalProperties": false,
	"required": [
		"username",
		"password"
	]
}