{
    "$schema": "http://json-schema.org/draft-06/schema#",
	"type": "object",
	"properties": {
		"username":  { "$ref": "#/$defs/usernameString" },
		"firstName": { "$ref": "#/$defs/nameString" },
		"lastName":  { "$ref": "#/$defs/nameString" },
		"birthdate": {
			"type":"string",
			"format": "date"
		},
		"email": {
			"type": "string",
			"minLength": 6,
			"maxLength": 60,
			"format":"email"
		},
		"password": {
			"type": "string",
			"minLength": 6,
			"maxLength": 20
		}
	},
	"additionalProperties": false,
	"required": [
		"username",
		"firstName",
		"lastName",
		"birthdate",
		"email",
		"password"
	],
	"$defs":{
		"usernameString": {
			"type": "string",
			"minLength": 4,
			"maxLength": 32
		},
		"nameString":{
			"type": "string",
			"minLength": 1,
			"maxLength": 32
		}
	}
}